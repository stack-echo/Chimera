from rpc import rag_pb2_grpc, rag_pb2
from workflows.chat_flow import ChatWorkflow # 🔥 新增：LangGraph 编排逻辑
from core.telemetry.tracing import trace_agent
from workflows.kg_builder.state import KGBuilderState
import time
import json
from opentelemetry import trace
from google.protobuf.message import Message
from google.protobuf.json_format import MessageToDict
import grpc

class ChimeraRagService(rag_pb2_grpc.RagServiceServicer):
    def __init__(self, nebula_store):
        # 🔥 核心修复：接收并保存 nebula_store
        self.nebula_store = nebula_store
        # 初始化工作流编排器
        self.chat_workflow = ChatWorkflow(nebula_store)
        print("✅ ChimeraRagService initialized with NebulaStore")

    @trace_agent("gRPC_Entry") # 记录入口 Payload
    def Chat(self, request, context):
        # 1. 组装初始状态
        initial_state = {
            "query": request.query,
            "kb_id": request.kb_id,
            "history": [], # 从 memory 模块获取
            "trace_id": context.invocation_metadata(), # 透传追踪 ID
        }

        # 2. 调用编排好的智能体工作流 (v0.5.0 的核心)
        workflow = ChatWorkflow()
        result = workflow.run(initial_state)

        # 3. 返回响应
        return rag_pb2.ChatResponse(answer=result["answer"])

    @trace_agent("gRPC:ChatStream")
    def ChatStream(self, request, context):
        # 1. 获取当前 SigNoz 链路的 Trace ID
        current_span = trace.get_current_span()
        trace_id = format(current_span.get_span_context().trace_id, "032x")

        print(f"DEBUG: 收到请求 Query='{request.query}', OrgID={request.org_id}, KbID={request.kb_id}")

        try:
            # 步骤 A: 初始反馈 (告知系统已准备就绪)
            yield rag_pb2.ChatReply(
                debug_info=rag_pb2.AgentMeta(
                    agent_name="System_Gateway",
                    thought="正在连接 Chimera 知识大脑，激活多智能体协作链路...",
                    trace_id=trace_id
                )
            )

            # 步骤 B: 模拟工作流中的不同智能体阶段
            # 实际开发中，这里会调用 self.chat_workflow.app.stream(...)

            # --- 阶段 1: 语义解析与关键词提取 ---
            time.sleep(0.5) # 模拟耗时
            yield rag_pb2.ChatReply(
                debug_info=rag_pb2.AgentMeta(
                    agent_name="Intent_Agent",
                    thought=f"已识别用户意图。正在尝试从知识库 {request.kb_id} 中检索关联实体...",
                    trace_id=trace_id
                )
            )

            # --- 阶段 2: 知识召回 (Graph + Vector) ---
            # 这里的亮点是向前端展示它在“查图”
            time.sleep(0.8)
            yield rag_pb2.ChatReply(
                source_docs=json.dumps([{"id": "doc_001", "title": "NebulaGraph_Handbook.pdf"}]),
                debug_info=rag_pb2.AgentMeta(
                    agent_name="Graph_Retriever",
                    thought="正在执行 nGQL 语句提取实体的二度关联关系...",
                    trace_id=trace_id
                )
            )

            # --- 阶段 3: 流式生成最终回答 ---
            # 模拟 LLM 逐字吐出
            full_answer = f"根据我的分析，知识库 {request.kb_id} 中关于 '{request.query}' 的记录显示：系统运行正常。"
            for i in range(0, len(full_answer), 5):
                chunk = full_answer[i:i+5]
                yield rag_pb2.ChatReply(answer_delta=chunk)
                time.sleep(0.1)

            # 步骤 C: 发送结束包，并给出追问建议
            yield rag_pb2.ChatReply(
                is_final=True,
                suggestions=["如何优化 NebulaGraph 的查询性能？", "查看当前 Trace 详情"]
            )

        except Exception as e:
            # 错误时，确保在 SigNoz 中记录异常并反馈给前端
            current_span.record_exception(e)
            yield rag_pb2.ChatReply(
                answer_delta=f"\n❌ 系统异常: {str(e)}",
                is_final=True
            )

    def BuildKG(self, request, context):
        # 实例化工作流
        builder = KGBuildWorkflow(self.nebula_store)
        # 启动异步流水线
        builder.app.invoke({"raw_text": request.content})
        return rag_pb2.Empty()
