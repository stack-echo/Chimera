from langgraph.graph import StateGraph, END
from runtime.state import AgentState
from core.telemetry.tracing import trace_agent

class ChatWorkflow:
    def __init__(self, nebula_store):
        self.nebula_store = nebula_store

        # 初始化状态机
        workflow = StateGraph(AgentState)

        # 定义节点
        workflow.add_node("memory_node", self.retrieve_memory)
        workflow.add_node("retrieval_node", self.parallel_retrieval)
        workflow.add_node("synthesis_node", self.generate_answer)

        # 连线：开始 -> 查记忆 -> 查库 -> 生成 -> 结束
        workflow.set_entry_point("memory_node")
        workflow.add_edge("memory_node", "retrieval_node")
        workflow.add_edge("retrieval_node", "synthesis_node")
        workflow.add_edge("synthesis_node", END)

        self.app = workflow.compile()

    @trace_agent("Memory_Node")
    def retrieve_memory(self, state: AgentState):
        # TODO: 从 Redis 捞取最近对话
        return {"short_term_context": "用户之前询问过 v0.4.0 部署问题"}

    @trace_agent("Retrieval_Node")
    def parallel_retrieval(self, state: AgentState):
        # 🔥 亮点：这里可以同时启动 NebulaGraph 和 Qdrant 的检索
        # 针对长尾知识，这里会去 NebulaGraph 探针
        return {
            "graph_context": "实体 Chimera 关联属性：Agentic Runtime",
            "vector_context": "匹配到文档切片：v0.5.0 支持 OTel 追踪"
        }

    @trace_agent("Node:Synthesis")
    def generate_answer(self, state: AgentState):
        # 从状态机中取出之前节点存入的知识
        v_context = state.get("vector_context", "无向量数据")
        g_context = state.get("graph_context", "无图谱数据")

        # 亮点：构建具备“全知视角”的 Prompt
        final_prompt = f"""
        你是一个具备图谱分析能力的 AI。请结合以下双路召回的信息回答：
        
        【知识图谱关联】：
        {g_context}
        
        【文档语义片段】：
        {v_context}
        
        【用户问题】：{state['query']}
        """

        # 渲染并调用 LLM
        response = self.llm.ask(final_prompt)
        return {"answer": response}

    def run(self, initial_input: dict):
        return self.app.invoke(initial_input)

    @trace_agent("Node:Graph_Search")
    def graph_search_node(state: AgentState):
        # 1. 提取 Query 中的关键词（实际可再接一个 NER Agent）
        keywords = extract_keywords(state["query"])

        # 2. 去图数据库捞取背景知识
        graph_knowledge = []
        for word in keywords:
            res = graph_skill.get_entity_relations(word)
            graph_knowledge.append(res)

        # 3. 将图知识存入状态，供后续 Synthesis 节点使用
        return {"graph_context": "\n".join(graph_knowledge)}

    @trace_agent("Node:Hybrid_Retriever")
    def hybrid_retrieval_node(self, state: AgentState):
        """
        亮点：同时调度向量检索和图检索，并在 SigNoz 展示对比
        """
        query = state["query"]

        # 1. 调用向量检索技能 (Qdrant)
        vector_docs = self.vector_skill.search(query)

        # 2. 调用图检索技能 (NebulaGraph)
        # 假设我们通过 NER 提取了实体
        graph_data = self.graph_skill.get_entity_context(query)

        # 3. 这里的亮点在于：SigNoz 会分别记录这两次 IO 的耗时和 Payload
        return {
            "vector_context": vector_docs,
            "graph_context": graph_data
        }
