import logging
import grpc
import os
import sys
from concurrent import futures
from config import Config
# 🔥 核心引入：OTel 拦截器
from opentelemetry.instrumentation.grpc import GrpcInstrumentorServer
from service.rag_service import ChimeraRagService
from core.stores.graph_store import NebulaStore
from core.telemetry.tracing import setup_otel

rpc_path = os.path.join(os.path.dirname(__file__), 'rpc')
if rpc_path not in sys.path:
    sys.path.insert(0, rpc_path)

from rpc import rag_pb2_grpc, rag_pb2

def serve():
    # 1. 启动 OTel 导出器 (必须最先运行)
    setup_otel(
        service_name=Config.SERVICE_NAME,
        endpoint=Config.OTEL_ENDPOINT
    )

    # 2. 初始化持久化存储 (用于注入 Service)
    nebula_store = NebulaStore(Config)

    # 3. 初始化 gRPC 服务端自动插桩
    # 注意：instrument() 必须在 grpc.server() 创建之前调用
    instrumentor = GrpcInstrumentorServer()
    if not instrumentor.is_instrumented_by_opentelemetry:
        instrumentor.instrument()

    # 4. 创建 Server
    server = grpc.server(
        futures.ThreadPoolExecutor(max_workers=getattr(Config, 'MAX_WORKERS', 10)),
        options=[
            ('grpc.max_send_message_length', Config.MAX_MESSAGE_LENGTH),
            ('grpc.max_receive_message_length', Config.MAX_MESSAGE_LENGTH),
        ]
    )

    # 5. 注册服务 (注入 nebula_store 依赖)
    # 这样你的 RagService 内部才能通过 self.nebula_store 调用图算法
    rag_pb2_grpc.add_RagServiceServicer_to_server(
        ChimeraRagService(nebula_store),
        server
    )

    server.add_insecure_port(f'[::]:{Config.PORT}')
    logging.info(f"🧠 Chimera Brain v0.5.0 (Agent Runtime) running on port {Config.PORT}...")
    server.start()
    server.wait_for_termination()

if __name__ == '__main__':
    logging.basicConfig(level=logging.INFO)
    serve()
