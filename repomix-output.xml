syntax = "proto3";

package rag.v1;

option go_package = "Chimera/api/rag/v1;v1";

// 定义 RAG 核心服务
service RagService {
  rpc ChatStream (ChatRequest) returns (stream ChatReply) {}
  rpc ParseAndIngest (ParseRequest) returns (ParseResponse) {}
}

// =========================================
// 聊天相关消息
// =========================================

message ChatRequest {
  string query = 1;
  string session_id = 2;
  int64 org_id = 3;
  int64 kb_id = 4;
  repeated Message history = 5;

  // 预留 1: 调试模式开关 (开启后会返回详细的 Agent 推理链)
  bool debug_mode = 6;
  // 预留 2: 动态模型配置 (允许针对单次请求指定特定的 LLM 参数)
  map<string, string> options = 7;
}

message Message {
  string role = 1; // "user" or "assistant"
  string content = 2;
}

message AgentMeta {
  string agent_name = 1;      // 当前正在工作的智能体 (如 NER_Agent)
  string thought = 2;         // 智能体的中间思考过程 (内省信息)
  int64 duration_ms = 3;      // 该步骤耗时
  string trace_id = 4;        // SigNoz 链路 ID
}

message ChatReply {
  string answer_delta = 1;
  string source_docs = 2;     // JSON 格式的引用列表

  // 预留 3: 观测增强数据
  AgentMeta debug_info = 3;
  bool is_final = 4;          // 流结束标志

  // 预留 4: 建议问题 (Agent 根据上下文自动生成的下一步追问)
  repeated string suggestions = 5;
}

// =========================================
// 解析相关消息 (ETL)
// =========================================

message ParseRequest {
  string storage_path = 1;
  string file_name = 2;
  int64 kb_id = 3;
  int64 doc_id = 4;
  int64 org_id = 5;

  // 预留 5: 解析选项 (例如是否开启 OCR, 是否强制重解析)
  map<string, string> params = 6;
}

message ParseResponse {
  bool success = 1;
  string error_msg = 2;

  // 🔥 变更 3: 只返回统计数据，不返回向量
  int32 chunk_count = 3; // 切了多少片
  int32 page_count = 4;  // 总页数 (Docling 解析结果)
  int32 token_usage = 5; // 消耗 Token 数 (可选)
}
